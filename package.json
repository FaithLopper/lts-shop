{
    "name": "eshop",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^13.5.0",
        "env-cmd": "^10.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "react-slick": "^0.29.0",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "beautiful": "prettier --single-quote --no-semi --tab-width 4 --trailing-comma all --write src/**/*.{js,json,scss}",
        "start": "env-cmd -f ./env/.local craco start",
        "build:dev": "env-cmd -f ./env/.dev craco build",
        "prebuild:prod": "rm -rf build",
        "build:prod": "env-cmd -f ./env/.prod craco build",
        "prebuild:staging": "rm -rf build",
        "build:staging": "env-cmd -f ./env/.staging craco build",
        "test": "craco test",
        "eject": "craco eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@ant-design/icons": "^4.7.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-brands-svg-icons": "^5.15.3",
        "@fortawesome/free-regular-svg-icons": "^6.2.0",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@reduxjs/toolkit": "^1.8.6",
        "antd": "^4.23.1",
        "antd-img-crop": "^4.2.4",
        "antd-mobile": "^5.24.2",
        "craco": "0.0.3",
        "craco-less": "^2.0.0",
        "i18next": "^21.9.1",
        "node-sass": "^6.0.0",
        "prettier": "^2.7.1",
        "query-string": "^7.1.1",
        "react-device-detect": "^2.2.2",
        "react-i18next": "^11.18.6",
        "react-otp-input": "^2.4.0",
        "react-redux": "^8.0.4",
        "react-router-dom": "^5.0.0",
        "redux": "^4.2.0",
        "redux-saga": "^1.2.1",
        "sass-globbing": "^1.0.0",
        "slick-carousel": "^1.8.1"
